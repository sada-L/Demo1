<Window xmlns="https://github.com/avaloniaui"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        mc:Ignorable="d" d:DesignWidth="300" d:DesignHeight="450" Width="300"
        x:Class="Demo.HistoryWindow"
		x:CompileBindings="False"
        Title="HistoryWindow">
	<Border>
		<ListBox Name="ListBox" >
			<ListBox.ItemTemplate>
				<DataTemplate>
					<Border BorderBrush="Black" BorderThickness="1" Padding="10">
						<StackPanel>
							<StackPanel Spacing="5">
								<TextBlock Text="{Binding Starttime}"/>
								<TextBlock Text="{Binding Countofdocs, StringFormat='всего файлов: {0}' }"/>
							</StackPanel>

							<ListBox ItemsSource="{Binding Documents}">
								<ListBox.ItemTemplate>
									<DataTemplate>
										<TextBlock Text="{Binding Documentpath}"/>
									</DataTemplate>
								</ListBox.ItemTemplate>
							</ListBox>
						</StackPanel>
					</Border>
				</DataTemplate>
			</ListBox.ItemTemplate>
		</ListBox>
	</Border>
</Window>
